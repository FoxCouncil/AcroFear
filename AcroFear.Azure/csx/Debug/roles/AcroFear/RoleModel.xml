<?xml version="1.0" encoding="utf-8"?>
<RoleModel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="AcroFear" version="2.7.6499.8" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition">
  <Startup>
    <Task commandLine="DiagnosticsPluginLauncher.exe" taskType="background" relativePath="plugins\Diagnostics">
      <Environment>
        <Variable name="IsEmulated" value="true" />
        <Variable name="RdModuleName" value="Diagnostics" />
        <Variable name="WaRoleType" value="IISWebRole" />
      </Environment>
    </Task>
    <Task commandLine="setup_web.cmd &gt; log.txt" executionContext="elevated" relativePath="approot\bin">
      <Environment>
        <Variable name="EMULATED">
          <RoleInstanceValue xpath="/RoleEnvironment/Deployment/@emulated" />
        </Variable>
        <Variable name="RUNTIMEID" value="node" />
        <Variable name="RUNTIMEURL" value="http://az413943.vo.msecnd.net/node/0.10.21.exe;http://nodertncu.blob.core.windows.net/iisnode/0.1.21.exe" />
        <Variable name="WaRoleType" value="IISWebRole" />
      </Environment>
    </Task>
  </Startup>
  <Runtime>
    <EntryPoint>
      <NetFxEntryPoint assemblyName="Microsoft.NodejsTools.WebRole.dll" targetFrameworkVersion="v4.0" />
    </EntryPoint>
  </Runtime>
  <Properties>
    <Property name="TargetFrameworkVersion" value="v4.0" />
    <Property name="RoleType" value="Web" />
    <Property name="RoleName" value="AcroFear" />
    <Property name="ProjectName" value="AcroFear" />
    <Property name="Configuration" value="Debug" />
    <Property name="Platform" value="AnyCPU" />
    <Property name="SetConfiguration" value="Configuration=Debug" />
    <Property name="SetPlatform" value="Platform=AnyCPU" />
    <Property name="RoleTargetFramework" value="v4.0" />
    <Property name="OutputDir" value="D:\Code\AcroFear\AcroFear.Azure\obj\Debug\AcroFear\" />
    <Property name="EntryPoint" value="Microsoft.NodejsTools.WebRole.dll" />
    <Property name="SourceDir" value="D:\Code\AcroFear\AcroFear\" />
    <Property name="SitePhysicalPath" value="D:\Code\AcroFear\AcroFear\" />
    <Property name="TargetRuntimeVersion" value="v4.0" />
  </Properties>
  <Sites>
    <Site name="Web" physicalDirectory="D:\Code\AcroFear\AcroFear\">
      <Bindings>
        <Binding name="Endpoint1" endpointName="Endpoint1" />
      </Bindings>
    </Site>
  </Sites>
</RoleModel>